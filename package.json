  },
  "dependencies": {
    "@astrojs/check": "^0.5.8",
    "@astrojs/mdx": "^2.2.0",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.6.0",
    "@iconify/types": "^2.0.0",
    "@sentry/astro": "^7.106.1",
    "@spotlightjs/astro": "^1.0.16",
    "astro": "^4.5.2",
    "astro-icon": "^1.1.0",
    "astro-tailwind-config-viewer": "^1.0.3",
    "class-variance-authority": "^0.7.0",
    "cssnano": "^6.1.0",
    "flowbite": "^2.3.0",
